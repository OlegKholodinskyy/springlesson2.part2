<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <import resource="storage-cofiguration.xml"/>


    <bean id="file_1" class="lesson3_HW.Beans.File">
        <constructor-arg name="name" value="TestFile"/>
        <constructor-arg name="format" value="jpg"/>
        <constructor-arg name="size" value="1000"/>
        <constructor-arg name="storage" ref="storage_1"/>

    </bean>

    <bean id="file_2" class="lesson3_HW.Beans.File">
        <constructor-arg name="name" value="TestFile_2"/>
        <constructor-arg name="format" value="doc"/>
        <constructor-arg name="size" value="1000"/>
        <constructor-arg name="storage" ref="storage_1"/>

    </bean>

    <bean id="file_3" class="lesson3_HW.Beans.File">
        <constructor-arg name="name" value="TestFile_3"/>
        <constructor-arg name="format" value="xls"/>
        <constructor-arg name="size" value="1000"/>
        <constructor-arg name="storage" ref="storage_2"/>

    </bean>

</beans>