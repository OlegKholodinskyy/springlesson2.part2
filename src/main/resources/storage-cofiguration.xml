<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean id="storage_1" class="lesson3_HW.Beans.Storage">
        <constructor-arg name="storageSize" value="1000"/>
        <constructor-arg name="storageCountry" value="USA"/>
        <constructor-arg name="formatsSupported" value="jpg"/>
    </bean>

    <bean id="storage_2" class="lesson3_HW.Beans.Storage">
        <constructor-arg name="storageSize" value="1000"/>
        <constructor-arg name="storageCountry" value="Default"/>
        <constructor-arg name="formatsSupported" value="jpg"/>
    </bean>




</beans>